    "@emotion/styled": "^11.11.0",
    "@formatjs/intl-localematcher": "^0.4.0",
    "@hookform/resolvers": "^3.3.1",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.14.3",
    "@rarimo/client": "^2.0.0-rc.17",
    "graphql": "^16.7.1",
    "graphql-tag": "^2.12.6",
